# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0

# Database (Optional - UI module can work with minimal DB)
DATABASE_URL=postgresql://user:password@localhost:5432/instagram_ui

# API Services Configuration
MANAGEMENT_API_URL=http://localhost:8089
WORKER_API_URL=http://localhost:8088
MONITORING_API_URL=http://localhost:8090

# API Authentication Tokens
API_TOKEN_MANAGEMENT=your-management-api-token
API_TOKEN_WORKER=your-worker-api-token
API_TOKEN_MONITORING=your-monitoring-api-token

# Monitoring Settings
MONITORING_REFRESH_INTERVAL=30

# Security (Optional)
SECURE_PROXY_SSL_HEADER=0
USE_X_FORWARDED_HOST=1